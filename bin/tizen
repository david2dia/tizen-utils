#!/usr/bin/env node

var tizen = require('../lib/tizen-utils');
var log = require('../lib/log');
var method = process.argv[2];
var args = process.argv.slice(3);

if (tizen[method]) {
    tizen[method](args[0], args[1]);
} else {
    log('Usage: tizen [method] [arguments]');
    log('tizen wgt dirPath wgtName');
    log('tizen install pathToWgt');
    log('tizen uninstall wgtId');
    log('tizen launch wgtId');
    log('tizen close wgtId');
    log('tizen list');
}
